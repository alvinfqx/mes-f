
@{
    ViewBag.Title = "SFC00020";
    Layout = "~/Views/Shared/_LayoutLang.cshtml";
}


@* 制令生产计划表 *@
<div id="tb_list" class="J-main">
    <div id="container" class="J-toolbar"></div>
    <div class="J-search" style="width:1000px">
        <lable data-meaning="StartOrderNumber">Start Order Number</lable>
        <input type="text" class="search-input" id="StartOrderNumber" @*data-bind="value:StartOrderNumber"*@ style="width: 120px;" />
        <input type="text" class="search-input" id="StartOrderNumberID" style="width: 120px;display:none;"/>
        @*<input type="text" class="search-input" id="StartSequence" data-bind="value:StartOrderSequence" style="width: 30px;display:none;" autocomplete="off" />*@
        <button type="button" class="btn btn-success btn-xs" onclick="model.StartOrderNumber('#StartOrderNumber', '#StartSequence')" style="margin-right:4px;"><span class="fa fa-desktop"></span></button>

        <lable data-meaning="EndOrderNumber">End Order Number</lable>
        <input type="text" class="search-input" id="EndOrderNumber" @*data-bind="value:EndOrderNumber"*@ style="width: 120px;" />
        <input type="text" class="search-input" id="EndOrderNumberID" style="width: 120px;display:none;" />
        @*<input type="text" class="search-input" id="EndSequence" data-bind="value:EndOrderSequence" style="width: 30px;display:none;" autocomplete="off" />*@
        <button type="button" class="btn btn-success btn-xs" onclick="model.StartOrderNumber('#EndOrderNumber', '#EndSequence')" style="margin-right:4px;"><span class="fa fa-desktop"></span></button>
   
        <lable data-meaning="ProcessStartDate" style="margin:0 5px;">Start Process Date</lable>
        <input id="SStartDate" type="text" class="search-input td_left" style="width:140px;" autocomplete="off" />
        <lable data-meaning="ProcessEndDate" style="margin:0 5px;">End Process Date</lable>
        <input id="EStartDate" type="text" class="search-input td_left" style="width:140px;" autocomplete="off" />
    
        <div id="secondRow" style="margin-top:8px;">
            <lable data-meaning="Status" style="display:inline-block;width:68px;text-align:center;">Status</lable>&nbsp;
            <div id="Status" data-bind="value:Status" style="display: inline-block; border-radius:4px;padding: 3px 5px; border:1px solid #ccc"></div>
            <input id="SStatus" style="display: none;">
        </div>
     </div>
   
    <div class="J-content" id="table-body">
        <table id="SFC00020Table"></table>
    </div>
    <div class="datagrid-pager page" id="paginagionBar"></div>
</div>

@* 制令单号弹窗 *@
<div class="modal inmodal" id="CodeDialog" tabindex="-1" role="dialog" aria-hidden="true" data-backdrop="static" data-keyboard="false">
    <div class="modal-dialog" style="width:800px;">
        <div class="modal-content animated bounceInRight">
            <div class="modal-header" style="padding:16px !important">
                <button id="close" type="button" class="close" onclick="model.CleanClick()">
                    <span aria-hidden="true">&times;</span><span class="sr-only" data-meaning="Close">Close</span>
                </button>
                <h6 class="modal-title" data-meaning="ProcessOrderNo">Process Order No.</h6>
            </div>
            <div class="J-search">
                <span data-meaning="MoNo" style="margin:5px 5px 8px 10px; width:60px;">Mo No</span>
                <input id="TxtMoNoSearch" type="text" class="search-input" style="width: 160px; margin:5px 5px 8px 0;" autocomplete="off" />
                <span data-meaning="ItemDescription" style="margin:5px 5px 8px 10px; width:60px;">Mo No</span>
                <input id="ItemDescription" type="text" class="search-input" style="width: 160px; margin:5px 5px 8px 0;" autocomplete="off" />
                <button class="btn btn-success btn-sm" type="button" onclick="model.DialogMoNoSearch()" data-meaning="Search" value="Search">Search</button>
            </div>
            <div id="table-bodyMono" style="margin:0 15px;">
                <table id="MoNoTable"></table>
            </div>
            <div class="datagrid-pager page" id="paginagionMoNoBar" style="margin:0 15px;"></div>
            <div class="modal-footer" style="text-align:right;">
                <button class="btn btn-success btn-sm" type="button" id="cancel" data-meaning="Cancel" value="Cancel" onclick="model.CleanClick()">Cancel</button>
                <button class="btn btn-success btn-sm" type="button" id="MoNoCommit" data-meaning="Comfirm" value="Comfirm">Commit</button>
            </div>
        </div>
    </div>
</div>




@section scripts{
    <style>
        .mf-table-wrapper {
            overflow-x: auto;
        }

    </style>
    
    <link href="~/Content/css/plugins/iCheck/blue.css" rel="stylesheet" />
    <link href="~/Content/js/datetimepicker/bootstrap-datetimepicker.css" rel="stylesheet" />
    <script src="~/Content/js/hplus/plugins/iCheck/icheck.min.js"></script>
    <script src="~/Content/js/bootstrap-datepicker/locales/bootstrap-datepicker.zh-CN.min.js"></script>
    <script src="~/Content/js/bootstrap-datepicker/locales/bootstrap-datepicker.zh-TW.min.js"></script>
    <script src="~/Content/js/knockout/knockout-3.0.0.js" type="text/javascript"></script>
    <script src="~/Content/js/current/pagination.js" type="text/javascript"></script>
    <script src="~/Content/js/current/Rander.js" type="text/javascript"></script>
    <script src="~/Content/js/current/Checker.js" type="text/javascript"></script>

    <script src="~/ViewModels/MES/IntelligentManufacturing/SFC00020.js" type="text/javascript"></script>
}
