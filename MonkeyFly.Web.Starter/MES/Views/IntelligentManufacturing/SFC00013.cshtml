
@{
    ViewBag.Title = "SFC00013";
    Layout = "~/Views/Shared/_LayoutLang.cshtml";
}

@*主页面表格*@
<div id="tb_list" class="J-main">
    <div id="container" class="J-toolbar">
    </div>
    <div class="J-search">
        <div style="margin:5px 5px;">
            <span data-meaning="StartProcessNo" style="margin:0 5px;">Start Process No.</span>
            <input id="StartProcessNo" type="text" class="search-input td_left" style="width:170px;" autocomplete="off" />
            <button type="button" class="btn btn-success btn-xs" onclick="model.showStartProcessNo()" style="margin-right:4px;"><span class="fa fa-desktop"></span></button>
            <span data-meaning="EndProcessNo" style="margin:0 5px;">End Process No.</span>
            <input id="EndProcessNo" type="text" class="search-input td_left" style="width:170px;" autocomplete="off" />
            <button type="button" class="btn btn-success btn-xs" onclick="model.showEndProcessNo()" style="margin-right:4px;"><span class="fa fa-desktop"></span></button>
            <span data-meaning="StartItemCode" style="margin:0 5px;">Start Item Code</span>
            <input id="StartItemCode" type="text" class="search-input td_left" style="width:170px;" autocomplete="off" />            
            <button type="button" class="btn btn-success btn-xs" onclick="model.showStartItemCode()" style="margin-right:4px;"><span class="fa fa-desktop"></span></button>
        </div>
        <div style="margin:5px 5px 0px 5px;">
            <span data-meaning="EndItemCode" style="margin:0 5px;">End Item Code</span>
            <input id="EndItemCode" type="text" class="search-input td_left" style="width:170px;" autocomplete="off" />
            <button type="button" class="btn btn-success btn-xs" onclick="model.showEndItemCode()" style="margin-right:4px;"><span class="fa fa-desktop"></span></button>
            <span data-meaning="StartDateOfCompletion" style="margin:0 5px;">Start Completion Date</span>
            <input id="StartDateOfCompletion" type="text" class="search-input td_left" style="width:170px;" autocomplete="off" />
            <span data-meaning="EndDateOfCompletion" style="margin:0 5px;">End Completion Date</span>
            <input id="EndDateOfCompletion" type="text" class="search-input td_left" style="width:170px;" autocomplete="off" />
        </div>
    </div>
    <div class="J-content" id="table-body">
        <table id="SFC00013Table" data-toggle="true" data-mobile-responsive="true"></table>
    </div>
    <div class="datagrid-pager page" id="paginagionBar"></div>
</div>

@*起始製程代號弹窗列表*@
<div class="modal inmodal" id="StartProcessNoDialog" tabindex="-1" role="dialog" aria-hidden="true" data-backdrop="static" data-keyboard="false">
    <div class="modal-dialog" style="width:570px;">
        <div class="modal-content animated bounceInRight">
            <div class="modal-header" style="padding:16px !important">
                <button id="close" type="button" class="close" data-dismiss="modal" onclick="model.clearInput('#StartProcessCode')">
                    <span aria-hidden="true">&times;</span><span class="sr-only" data-meaning="Close">Close</span>
                </button>
                <h6 class="modal-title" data-meaning="ProcessMaster">Process Master</h6>
            </div>
            <div class="J-search">
                <span data-meaning="ProcessNo" style="margin:5px 5px 8px 10px; width:60px;">Process No.</span>
                <input id="StartProcessCode" type="text" class="search-input" style="width: 160px; margin:5px 5px 8px 0;" autocomplete="off" />
                <button class="btn btn-success btn-sm" type="button" onclick="model.StartProcessCodeSearch()" id="Find" data-meaning="Search" value="Search">Search</button>
            </div>
            <div style="margin:0 15px 5px 15px;">
                <table id="StartProcessNoTable"></table>
            </div>
            <div class="datagrid-pager page" id="paginagionStartProcessNoBar" style="margin:0 10px;"></div>
            <div class="modal-footer" style="text-align:right; padding:8px 15px;">               
                <button class="btn btn-success btn-sm" type="button" data-dismiss="modal" id="cancel" onclick="model.clearInput('#StartProcessCode')" data-meaning="Cancel" value="Cancel">Cancel</button>
                <button class="btn btn-success btn-sm" type="button" id="StartProcessNoCommit" data-meaning="Comfirm" onclick="model.clearInput('#StartProcessCode')" value="Comfirm">Commit</button>
            </div>
        </div>
    </div>
</div>

