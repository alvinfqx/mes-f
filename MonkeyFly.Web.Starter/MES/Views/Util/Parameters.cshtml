    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewBag.LanguageURL = "/Data/Language/EMO/Util/Parameters";
    ViewBag.TipURL = "";
}

@*参数通用模块*@
<div id="tb_list" class="J-main">
    <div style="height:99.29%;">
