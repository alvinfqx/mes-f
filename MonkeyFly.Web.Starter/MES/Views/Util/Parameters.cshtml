            <div style="padding: 6px;font-weight: 600;border: 1px solid #e4eaec;">
                <span data-meaning="Parameter Type">Parameter Type</span>
            </div>
            <div style="border-left:1px solid #e4eaec;border-right:1px solid #e4eaec;border-bottom:1px solid #e4eaec;height:92.8%;overflow-y: auto;">
                <div id="TypeTree"></div>
            </div>
        </div>
        <div style="display: inline-block;width: 80%;height: 100%;">
            <iframe id="iframe" style="width: 100%;height: 99.9%;border: none;"></iframe>
        </div>
    </div>
</div>

@section scripts{
