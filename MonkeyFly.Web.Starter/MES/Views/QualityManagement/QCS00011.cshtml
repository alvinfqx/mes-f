@*检验群组码设定*@
<div id="tb_list" class="J-main">
    <div id="container" class="J-toolbar"></div>
    <div class="J-search">
        <div>
            <span data-meaning="StartComplaintNoSeq"></span>
            <input id="SComplaintNoSeq" name="SComplaintNoSeq" type="text" data-bind="value:SComplaintNoSeq" class="search-input" style="width: 170px;" autocomplete="off" />
            <button class="btn btn-success btn-xs" type="button" onclick="model.ComplaintNoSeqSearch('#SComplaintNoSeq')" style="margin-right:4px;" value="Search"><span class="fa fa-desktop"></span></button>
            <span data-meaning="EndComplaintNoSeq"></span>
            <input id="EComplaintNoSeq" name="EComplaintNoSeq" type="text" class="search-input" style="width: 170px;" autocomplete="off" />
            <button class="btn btn-success btn-xs" type="button" onclick="model.ComplaintNoSeqSearch('#EComplaintNoSeq')" style="margin-right:4px;" value="Search"><span class="fa fa-desktop"></span></button>
            <span data-meaning="StartDate"></span>
            <input id="SDate" name="SDate" type="text" class="search-input" style="width: 100px;" autocomplete="off" />
            <span data-meaning="EndDate"></span>
            <input id="EDate" name="EDate" type="text" class="search-input" style="width: 100px;" autocomplete="off" />
        </div>
        <div style="padding-top:5px;">
            <span data-meaning="StartCustomerNo"></span>
            <input id="SCstNo" name="SCstNo" type="text" class="search-input" style="width: 170px;" autocomplete="off" />
            <button class="btn btn-success btn-xs" type="button" onclick="model.CstNoSearch('#SCstNo')" style="margin-right:4px;" value="Search"><span class="fa fa-desktop"></span></button>
            <span data-meaning="EndCustomerNo"></span>
            <input id="ECstNo" name="ECstNo" type="text" class="search-input" style="width: 170px;" autocomplete="off" />
            <button class="btn btn-success btn-xs" type="button" onclick="model.CstNoSearch('#ECstNo')" style="margin-right:4px;" value="Search"><span class="fa fa-desktop"></span></button>
            &nbsp;
            <span data-meaning="Status">Status</span>&nbsp;
            <div id="SStatus" data-bind="value:Status" style="display: inline-block; padding: 3px 5px; border:1px solid #ccc"></div> 
            @*<span data-meaning="Status"></span>
            <input id="NA" name="NA" type="checkbox" class="iCheck" data-meaning="NA" />
            <span data-meaning="NA"></span>
            <input id="OP" name="OP" type="checkbox" class="iCheck" data-meaning="OP" />
            <span data-meaning="OP"></span>
            <input id="CL" name="CL" type="checkbox" class="iCheck" data-meaning="CL"/>
            <span data-meaning="CL"></span>
            <input id="CA" name="CA" type="checkbox" class="iCheck" data-meaning="CA" />
            <span data-meaning="CA"></span>*@
        </div>
    <div class="J-content" id="table-body">
        <table id="ComplaintTable"></table>
    </div>
    <div class="datagrid-pager page" id="paginagionBar"></div>
    </div>
</div>

@*查看附件*@
<div class="modal inmodal" id="ViewAttachmentsDialog" tabindex="0" role="dialog" aria-hidden="true" data-backdrop="static" data-keyboard="false">
    <div class="modal-dialog" style="width:850px;">
        <div class="modal-content animated bounceInRight">
            <div class="modal-header" style="padding:5px 10px 5px 5px  !important">
                <button type="button" class="close" data-dismiss="modal">
                    <span aria-hidden="true">&times;</span><span class="sr-only">Shut</span>
                </button>
                <h6 class="modal-title" data-meaning="ViewAttachments">View Attachments</h6>
            </div>
            <div class="modal-body" style="padding: 0px 5px 2px!important" id="table_body_process_data">
                <table id="ViewAttachmentsTable" data-toggle="true" data-mobile-responsive="true"></table>
            </div>
            <div id="paginagionViewAttachmentsBar" class="datagrid-pager page" style="padding-bottom:5px!important"></div>
            <div class="modal-footer" style="padding: 5px !important">
                <button type="button" class="btn btn-success btn-sm" data-dismiss="modal" data-meaning="Close">Close</button>
            </div>
        </div>
    </div>
</div>

