@section scripts{
    <style>
        .mf-table-wrapper {
            overflow-x: auto;
        }

        #table-body .fix-table, #table-body .fix-head {
            width: 300%;
        }
        
         #table_body_process_data .fix-table, #table_body_process_data .fix-head {
            width: 100%;
        }

        #table-bodyData .fix-table, #table-bodyData .fix-head {
            width: 220%;
        }
        select {
            height: 25.5px;
            font-size: 12px;
            color: #555;
            background-color: #fff;
            background-image: none;
            border: 1px solid #ccc;
            border-radius: 4px;
            -webkit-box-shadow: inset 0 1px 1px rgba(0,0,0,.075);
            box-shadow: inset 0 1px 1px rgba(0,0,0,.075);
            -webkit-transition: border-color ease-in-out .15s,-webkit-box-shadow ease-in-out .15s;
            -o-transition: border-color ease-in-out .15s,box-shadow ease-in-out .15s;
            transition: border-color ease-in-out .15s,box-shadow ease-in-out .15s;
        }
        #ComplaintNoSeqDialog .fix-table, #ComplaintNoSeqDialog .fix-head{
            width:100%;
        }
</style>
