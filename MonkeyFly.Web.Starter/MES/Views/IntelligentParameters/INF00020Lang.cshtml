
@{
    ViewBag.Title = "INF00020Lang";
    Layout = "~/Views/Shared/_LayoutLang.cshtml";
}

@*系统参数維護*@
<div id="tb_list" class="J-main">
    <div id="container" class="J-toolbar">
        <button class="btn btn-success btn-sm" onclick="model.backClick()"><i class="fa icon-arrow-left"></i> <span data-meaning="Cancel">Cancel</span></button>
        <button class="btn btn-success btn-sm" onclick="model.addClick()"><i class="fa icon-add"></i> <span data-meaning="Add">Add</span></button>
        <button class="btn btn-success btn-sm" onclick="model.editClick()"><i class="fa icon-edit"></i> <span data-meaning="Change">Edit</span></button>
        <button class="btn btn-success btn-sm" onclick="model.deleteClick()"><i class="fa icon-delete"></i> <span data-meaning="Delete">Delete</span></button>
        <button class="btn btn-success btn-sm" onclick="model.comfirmClick()"><i class="fa icon-save"></i> <span data-meaning="Comfirm">Comfirm</span></button>
    </div>
    <div class="J-search">
    </div>
    <div class="J-content" id="table-body">
        <table id="table" data-toggle="true" data-mobile-responsive="true"></table>
    </div>
    <div class="datagrid-pager page" id="paginagionBar"></div>
</div>

@section scripts{
    <style>
        #table-body .mf-table-wrapper {
            overflow-x: auto;
        }

        #table-body .fix-table, #table-body .fix-head {
            width: 110%;
        }

    </style>
    <script src="~/Content/js/knockout/knockout-3.0.0.js" type="text/javascript"></script>
    <script src="~/Content/js/current/pagination.js" type="text/javascript"></script>
    <script src="~/Content/js/current/Rander.js" type="text/javascript"></script>
    <script src="~/Content/js/current/Checker.js" type="text/javascript"></script>
    <script src="~/ViewModels/MES/IntelligentParameters/INF00020Lang.js" type="text/javascript"></script>
}
