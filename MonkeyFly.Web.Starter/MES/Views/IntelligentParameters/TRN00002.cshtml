
@{
    ViewBag.Title = "MES資料轉入處理-主檔";
    Layout = "~/Views/Shared/_LayoutLang.cshtml";

}

<div id="tb_list" class="J-main">
    <div id="container" class="J-toolbar"></div>
    <div class="J-search">
        <label data-meaning="StartDate">Start Date</label>
        <input type="text" id="StartDate"  class="search-input" data-bind="value:StartDate" style="width: 130px;" autocomplete="off" />
        <label data-meaning="EndDate">End Date</label>
        <input type="text" id="EndDate" class="search-input" data-bind="value:EndDate" style="width: 130px;" autocomplete="off" />
        <label data-meaning="StartMasterCode">Start Master Code</label>
        <input type="text" class="search-input" data-bind="value:StartMasterCode" style="width: 130px;" autocomplete="off" />
        <label data-meaning="EndMasterCode">End Master Code</label>
        <input type="text" class="search-input" data-bind="value:EndMasterCode" style="width: 130px;" autocomplete="off" />
        <label data-meaning="MasterCategory">MasterCategory</label>
        <input type="text" class="search-input" data-bind="value:Type" style="width: 130px;" autocomplete="off" />
    </div>
    <div class="J-content" id="table-body">
        <table id="TRN00002Table" data-toggle="true" data-mobile-responsive="true"></table>
    </div>
    <div class="datagrid-pager page" id="paginationBar"></div>
</div>

@section scripts{

    <style>
        #table-body .mf-table-wrapper {
            overflow-x: auto;
        }
    </style>
    <script src="~/Content/js/knockout/knockout-3.0.0.js" type="text/javascript"></script>
    <script src="~/Content/js/current/koCustomBinding.js"></script>
    <script src="~/Content/js/current/pagination.js" type="text/javascript"></script>
    <script src="~/Content/js/current/Rander.js" type="text/javascript"></script>
    <script src="~/Content/js/current/Checker.js" type="text/javascript"></script>
    <script src="~/Content/js/current/FrozenColumn.js" type="text/javascript"></script>
    <script src="~/ViewModels/MES/IntelligentParameters/TRN00002.js" type="text/javascript"></script>

}
