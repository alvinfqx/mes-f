@*菜单列表*@
<div id="tb_list" class="J-main">
    <div id="container" class="J-toolbar"></div>
    <div class="J-content">
        <table id="MenuTable" data-toggle="true" data-mobile-responsive="true"></table>
    </div>
</div>

@*新增*@
<div class="modal inmodal" id="AddDialog" tabindex="-1" role="dialog" aria-hidden="true">
    <style>
        .ui-state-highlight, .ui-widget-content .ui-state-highlight, .ui-widget-header .ui-state-highlight {
            border: 1px solid red;
        }
    </style>
    <div class="modal-dialog" style="width:800px;">
        <div class="modal-content animated bounceInRight">
            <div class="modal-header" style="padding:16px !important">
                <button type="button" class="close" data-dismiss="modal">
                    <span aria-hidden="true">&times;</span><span class="sr-only">关闭</span>
                </button>
                <h6 class="modal-title">添加导航菜单</h6>
            </div>
            <div style="height:420px;background:#fff;padding:10px 0px 10px 50px;">
                <div style="width: 208px; height: 400px; display:inline-block;border: 1px solid #e4eaec;">
                    <div style="width:100%;height:30px;padding-left:10px;border-bottom: 1px solid #e4eaec;">
                        <span style="line-height:30px;">菜单导航</span>
                    </div>
                    <div style="padding: 0px; overflow: auto;width:100%;height:370px">
                        <div id="AddMenuTree"></div>
                    </div>
                </div>
                <div style="width: 520px; height: 400px; display:inline-block;">
                    <div class="form-horizontal" style="padding: 0px; width:100%;height:100%">
                        <div class="form-group">
                            <label class="col-sm-3 control-label">* 菜单名称：</label>
                            <div class="col-sm-7">
                                <input id="addName" name="addName" type="text" class="form-control" data-bind="value:form.Name" placeholder="必须填写" />
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-sm-3 control-label">链接地址：</label>
                            <div class="col-sm-7">
                                <input id="addURL" name="addURL" class="form-control" data-bind="value:form.URL">
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-sm-3 control-label">排序：</label>
                            <div class="col-sm-7">
                                <input id="addSequence" name="addSequence" class="form-control" data-bind="value:form.Sequence" />
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-sm-3 control-label">上级菜单：</label>
                            <div class="col-sm-7">
                                <input id="addParentType" name="addParentType" type="text" class="form-control" required="" aria-required="true" readonly="readonly" placeholder="请选择上级菜单" data-bind="value:form.ParentName">
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-sm-3 control-label">图标：</label>
                            <div class="col-sm-7">
                                <input id="addIconClass" name="addIconClass" class="form-control" style="width: 84.6%; display: inline;" data-bind="value: form.IconClass" readonly="readonly" />
                                <button type="button" id="addSetIcon" class="btn btn-white" style="margin-top: -5px;"><i class="fa icon-cog"></i></button>
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-sm-3 control-label">是否可见：</label>
                            <div class="col-sm-7">
                                <select id="addIsVisible" name="addIsVisible" class="form-control" data-bind="options:[{ value: 1, text: '是' }, { value: 0, text: '否' }],optionsValue:'value',optionsText:'text',value:form.IsVisible"></select>
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-sm-3 control-label">是否启用：</label>
                            <div class="col-sm-7">
                                <select id="addIsEnable" name="addIsEnable" class="form-control" data-bind="options:[{ value: 1, text: '是' }, { value: 0, text: '否' }],optionsValue:'value',optionsText:'text',value:form.IsEnable"></select>
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-sm-3 control-label">是否参数：</label>
                            <div class="col-sm-7">
                                <select id="addIsParameter" name="addIsParameter" class="form-control" data-bind="options:[{ value: 1, text: '是' }, { value: 0, text: '否' }],optionsValue:'value',optionsText:'text',value:form.IsParameter"></select>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-white" data-dismiss="modal">关闭</button>
                <button type="button" class="btn btn-success" id="addSave">保存</button>
            </div>
        </div>
    </div>
</div>

